{
  "name": "nsecure",
  "version": "0.9.0",
  "description": "Node.js security CLI",
  "main": "./bin/index.js",
  "bin": {
    "node-secure": "./bin/index.js",
    "nsecure": "./bin/index.js"
  },
  "type": "module",
  "engines": {
    "node": ">=16.1.0"
  },
  "scripts": {
    "eslint": "eslint bin src test",
    "eslint-fix": "npm run eslint -- --fix",
    "prepublishOnly": "npm run build && pkg-ok",
    "build": "node ./esbuild.config.js",
    "test": "npm run test-only && npm run eslint",
    "test-only": "cross-env esm-tape-runner 'test/**/*.spec.js'",
    "coverage": "c8 --all --src ./src -r html npm run test"
  },
  "files": [
    "bin",
    "dist",
    "src",
    "views"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ES-Community/nsecure.git"
  },
  "keywords": [
    "node",
    "nodejs",
    "security",
    "cli",
    "sast",
    "scanner",
    "static",
    "code",
    "analysis",
    "node_modules",
    "tree",
    "npm",
    "registry",
    "graph",
    "visualization",
    "dependencies"
  ],
  "author": "ES Community",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ES-Community/nsecure/issues"
  },
  "homepage": "https://github.com/ES-Community/nsecure#readme",
  "devDependencies": {
    "@myunisoft/httpie": "^1.3.1",
    "@nodesecure/eslint-config": "^1.3.0",
    "@nodesecure/size-satisfies": "^1.0.2",
    "@nodesecure/vis-network": "^1.1.0",
    "@small-tech/esm-tape-runner": "^1.0.3",
    "@small-tech/tap-monkey": "^1.3.0",
    "@types/node": "^16.11.11",
    "c8": "^7.10.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.2",
    "eslint": "^8.4.0",
    "list.js": "^2.3.1",
    "pkg-ok": "^2.3.1",
    "pretty-bytes": "^5.6.0",
    "split2": "^4.1.0",
    "tape": "^5.3.2"
  },
  "dependencies": {
    "@nodesecure/flags": "^2.2.0",
    "@nodesecure/i18n": "^1.2.1",
    "@nodesecure/npm-registry-sdk": "^1.3.0",
    "@nodesecure/scanner": "^3.0.0",
    "@nodesecure/utils": "^1.0.0",
    "@nodesecure/vuln": "^1.4.1",
    "@polka/send-type": "^0.5.2",
    "@slimio/async-cli-spinner": "^0.5.2",
    "cliui": "^7.0.4",
    "dotenv": "^10.0.0",
    "filenamify": "^5.0.2",
    "kleur": "^4.1.4",
    "ms": "^2.1.3",
    "open": "^8.4.0",
    "polka": "^0.5.2",
    "qoa": "^0.2.0",
    "sade": "^1.7.4",
    "semver": "^7.3.5",
    "sirv": "^1.0.19",
    "zup": "0.0.1"
  }
}
